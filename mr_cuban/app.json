{
  "expo": {
    "name": "MR Cuban",
    "scheme": "mrcuban",
    "slug": "mrcuban",
    "version": "9.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gouravbajpaizxcv.mrcuban"
    },
    "android": {
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#FF231F7C"
      },
      "package": "com.gourav_bajpaizxcv.mrcuban"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF231F7C"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8dbc5365-2ef5-47fc-a377-837b42371d38"
      }
    },
    "owner": "sudhanshu_dev"
  }
}
